<h4>Elected Subjects</h4>
<br>
<%@subjects=@batch.electivegroups.first.subjects%>
<%=link_to "Add Elected Subjects",addelectivesubject_path(@batch.id), remote: true%>
<table class="table table-hover" style="width: 500px;">
<tr>
<th>Id</th><th>Subject Name</th><th>Subject Code</th></tr>	
<%@subjects.each do |f|%>
<tr>
	<td><%=f.id%></td><td><%=f.name%></td><td><%=f.code%></td>


</tr>
<%end%>
</table>


