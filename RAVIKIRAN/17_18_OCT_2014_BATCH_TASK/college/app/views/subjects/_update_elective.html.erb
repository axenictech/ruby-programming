<div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">UPDATE ELECTIVE SUBJECT DETAILS</h4>
            </div>
            <div class="modal-body">
<%= form_for @e, url:{action: 'elective_update',id:@e.id} do |f| %>
<table border=1>
	<tr>
		<td> <%= f.label :'Elective group :' %> </td>
		<td> <%= f.text_field :es_group %> </td>
	</tr>

	<tr>
		<td> <%= f.label :'subject name :' %> </td>
		<td> <%= f.text_field :es_subject  %> </td>
	</tr>
</table> <br>
<%= f.submit :"UPDATE" %>
<%end%>
</div>
</div>
</div>
